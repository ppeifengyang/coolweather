<litepal>
    <!--将数据库名指定成cool_weather-->
    <!--数据库版本指定成1-->
    <!--并将Province,City,County3个实体类添加到映射列表中-->
    <dbname value="cool_weather"/>
    <version value="1"/>
    <list>
        <mapping class="com.example.administrator.coolweather.db.Province"/>
        <mapping class="com.example.administrator.coolweather.db.City"/>
        <mapping class="com.example.administrator.coolweather.db.County"/>
    </list>
</litepal>